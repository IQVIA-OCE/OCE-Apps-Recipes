        "@testing-library/react-native": "^9.0.0",
        "apollo-react-native": "git+https://${USER_NAME}:${APOLLO_TOKEN}@gitlab.ims.io/apollo/apollo-react-native#14.0.0",
        "add": "^2.0.6",
        "color": "^4.2.3",
