{
  "name": "OCEPlugin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "mkdir -p ./ios; node node_modules/react-native/local-cli/cli.js bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios/'; rm -rf ios.zip; zip -r ios.zip ./ios;",
    "start": "yarn react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "*",
    "apollo-react-native": "git+https://${USER_NAME}:${APOLLO_TOKEN}@gitlab.ims.io/apollo/apollo-react-native#13.0.0",
    "jest": "26.6.3",
    "jest-file-snapshot": "0.3.8",
    "luxon": "1.25.0",
    "oce-apps-bridges": "13.0.4",
    "oce-apps-scripts": "^1.1.0",
    "react": "17.0.2",
    "react-native": "0.66.2",
    "react-native-vector-icons": "6.5.0",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "App.{js,jsx}",
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-native|victory-*|@react-native-community|apollo-react-native|oce-apps-bridges)"
    ],
    "setupFiles": [
      "./node_modules/oce-apps-bridges/lib/setupMocks.js",
      "./__mocks__/EnvironmentData.js"
    ]
  }
}
