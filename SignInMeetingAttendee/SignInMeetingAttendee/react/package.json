{
  "name": "OCEPlugin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "oce-apps-scripts build-ios",
    "start": "oce-apps-scripts start-ios",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "*",
    "@reduxjs/toolkit": "1.8.2",
    "@testing-library/react-native": "9.1.0",
    "apollo-react-native": "git+https://${USER_NAME}:${APOLLO_TOKEN}@gitlab.ims.io/apollo/apollo-react-native#16.0.0",
    "jest": "26.6.3",
    "jest-file-snapshot": "0.5.0",
    "luxon": "2.4.0",
    "oce-apps-bridges": "16.0.0",
    "oce-apps-scripts": "3.0.0",
    "react": "18.2.0",
    "react-native": "0.70.3",
    "react-native-signature-canvas": "4.3.1",
    "react-native-touch-id": "4.4.1",
    "react-native-vector-icons": "6.4.2",
    "react-native-webview": "11.23.1",
    "react-redux": "7.2.8",
    "react-test-renderer": "18.2.0",
    "babel-preset-expo": "9.2.0"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "App.{js,jsx}",
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-native|victory-*|@react-native-community|apollo-react-native|react-navigation|@react-navigation|react-naive-gesture-handler|oce-apps-bridges)"
    ],
    "setupFiles": [
      "./node_modules/oce-apps-bridges/lib/setupMocks.js",
      "./__mocks__/EnvironmentData.js",
      "./__mocks__/LayoutBridge.js"
    ]
  }
}
