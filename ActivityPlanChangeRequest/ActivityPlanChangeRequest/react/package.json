    "react-navigation-stack": "1.7.3",
    "react-redux": "7.2.6",
    "react-test-renderer": "17.0.2",
    "yup": "0.32.11",
    "oce-apps-scripts": "2.0.1",
    "oce-apps-bridges": "15.0.2"
