{
    "name": "OCEPlugin",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "oce-apps-scripts start-ios",
        "build": "oce-apps-scripts build-ios",
        "start-web": "oce-apps-scripts start-web",
        "build-web": "oce-apps-scripts build-web",
        "test": "jest"
    },
    "dependencies": {
        "@babel/core": "*",
        "@reduxjs/toolkit": "1.5.1",
        "@testing-library/react-hooks": "7.0.2",
        "@testing-library/react-native": "9.0.0",
        "apollo-react-native": "git+https://${USER_NAME}:${APOLLO_TOKEN}@gitlab.ims.io/apollo/apollo-react-native#13.0.0",
        "jest": "27.4.7",
        "jest-file-snapshot": "0.5.0",
        "luxon": "^2.0.1",
        "react": "17.0.2",
        "react-native": "0.66.2",
        "react-native-vector-icons": "6.5.0",
        "react-redux": "7.2.6",
        "react-test-renderer": "17.0.2",
        "oce-apps-scripts": "^1.1.0",
        "oce-apps-bridges": "13.0.4"
    },
    "jest": {
        "preset": "react-native",
        "collectCoverageFrom": [
            "App.{js,jsx}",
            "src/**/*.{js,jsx}",
            "!<rootDir>/node_modules/",
            "!<rootDir>/bridge/"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!@react-native|react-native|victory-*|@react-native-community|apollo-react-native|oce-apps-bridges)"
        ],
        "setupFiles": [
            "./node_modules/oce-apps-bridges/lib/setupMocks.js"
        ]
    },
    "devDependencies": {}
}
