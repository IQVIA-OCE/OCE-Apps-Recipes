{
  "name": "OCEPlugin",
  "description": "Order - Search for Orders",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "oce-apps-scripts build-ios",
    "build-web": "oce-apps-scripts build-web",
    "start": "oce-apps-scripts start-ios",
    "start-web": "oce-apps-scripts start-web",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "*",
    "@oce-apps/apollo-react-native": "alpha",
    "@oce-apps/oce-apps-bridges": "alpha",
    "@oce-apps/oce-apps-scripts": "alpha",
    "@react-native/metro-config": "0.75.2",
    "@reduxjs/toolkit": "1.8.6",
    "@testing-library/jest-native": "5.4.1",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/react-native": "12.6.1",
    "babel-preset-expo": "11.0.14",
    "jest": "29.7.0",
    "jest-file-snapshot": "0.5.0",
    "luxon": "3.0.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.75.2",
    "react-native-vector-icons": "6.4.2",
    "react-native-web": "0.19.12",
    "react-redux": "7.2.4",
    "react-test-renderer": "18.3.1"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "App.{js,jsx}",
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-native|victory-*|@react-native-community|@oce-apps/apollo-react-native|react-native-vector-icons|@oce-apps/oce-apps-bridges)"
    ],
    "setupFiles": [
      "./node_modules/@oce-apps/oce-apps-bridges/lib/setupMocks.js"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  }
}
