{
  "name": "OCEPlugin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "oce-apps-scripts build-ios",
    "build-web": "oce-apps-scripts build-web",
    "start": "oce-apps-scripts start-ios",
    "start-web": "oce-apps-scripts start-web",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "*",
    "@reduxjs/toolkit": "1.8.6",
    "@testing-library/jest-native": "5.4.1",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/react-native": "11.5.0",
    "apollo-react-native": "git+https://${USER_NAME}:${APOLLO_TOKEN}@gitlab.ims.io/apollo/apollo-react-native#${APOLLO_VERSION}",
    "babel-preset-expo": "9.2.0",
    "jest": "29.4.3",
    "jest-file-snapshot": "0.5.0",
    "luxon": "3.0.4",
    "oce-apps-bridges": "17.0.5",
    "oce-apps-scripts": "4.0.0",
    "react": "18.1.0",
    "react-native": "0.70.3",
    "react-native-vector-icons": "6.4.2",
    "react-redux": "7.2.4",
    "react-test-renderer": "18.2.0"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "App.{js,jsx}",
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-native|victory-*|@react-native-community|apollo-react-native|react-native-vector-icons|oce-apps-bridges)"
    ],
    "setupFiles": [
      "./node_modules/oce-apps-bridges/lib/setupMocks.js"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  }
}
