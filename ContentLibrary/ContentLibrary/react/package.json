{
  "name": "OCEPlugin",
  "description": "Content Library Widget",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "oce-apps-scripts start-ios",
    "build": "oce-apps-scripts build-ios",
    "test": "jest",
    "build-web": "oce-apps-scripts build-web",
    "start-web": "oce-apps-scripts start-web"
  },
  "dependencies": {
    "@babel/core": "*",
    "@react-navigation/core": "6.4.17",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/stack": "6.4.1",
    "@reduxjs/toolkit": "1.6.0",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/react-native": "12.6.1",
    "@oce-apps/apollo-react-native": "alpha",
    "babel-preset-expo": "11.0.14",
    "color": "3.1.2",
    "jest": "29.7.0",
    "jest-file-snapshot": "0.3.8",
    "luxon": "2.5.0",
    "@oce-apps/oce-apps-bridges": "alpha",
    "@oce-apps/oce-apps-scripts": "alpha",
    "react": "18.3.1",
    "react-native": "0.75.2",
    "react-native-gesture-handler": "2.18.1",
    "react-native-screens": "3.34.0",
    "react-native-vector-icons": "6.4.2",
    "react-redux": "7.2.4",
    "react-test-renderer": "18.3.1",
    "@react-native/metro-config": "0.75.2",
    "react-native-web": "0.19.12",
    "react-dom": "18.3.1"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "App.{js,jsx}",
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/bridge/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@react-native|react-native|victory-*|@react-native-community|@oce-apps/apollo-react-native|react-navigation|@oce-apps/oce-apps-bridges)"
    ],
    "setupFiles": [
      "./node_modules/@oce-apps/oce-apps-bridges/lib/setupMocks.js"
    ]
  }
}
