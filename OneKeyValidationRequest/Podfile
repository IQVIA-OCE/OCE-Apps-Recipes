install! 'cocoapods', :disable_input_output_paths => true

post_install do |installer|
    system("sh \"./OneKeyValidationRequest/Supporting Files/unzip-react.sh\" --pods-path=\"./Pods/OCE\"")
end

target 'OneKeyValidationRequest' do
  use_frameworks!
  pod 'OCE', :git => "https://${USER_NAME}:${OCE_TOKEN}@gitlab.ims.io/oce/oce-apps-framework", :tag => "7.0.7"
end
