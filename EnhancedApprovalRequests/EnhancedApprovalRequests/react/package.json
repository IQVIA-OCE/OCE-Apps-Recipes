{
  "name": "OCEPlugin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "oce-apps-scripts build-ios",
    "build-web": "oce-apps-scripts build-web",
    "start": "oce-apps-scripts start-ios",
    "start-web": "oce-apps-scripts start-web",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "*",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/react-native": "11.0.0",
    "apollo-react-native": "git+https://${USER_NAME}:${APOLLO_TOKEN}@gitlab.ims.io/apollo/apollo-react-native#15.0.1",
    "jest": "^26.6.3",
    "jest-file-snapshot": "0.5.0",
    "luxon": "3.0.1",
    "oce-apps-bridges": "15.0.2",
    "oce-apps-scripts": "2.0.1",
    "react": "17.0.2",
    "react-native": "0.66.2",
    "react-native-keyboard-spacer": "0.4.1",
    "react-native-vector-icons": "6.4.2",
    "react-redux": "^8.0.2",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "App.{js,jsx}",
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/bridge/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@react-native|react-native|victory-*|@react-native-community|apollo-react-native|oce-apps-bridges)"
    ],
    "setupFiles": [
      "./node_modules/oce-apps-bridges/lib/setupMocks.js"
    ]
  }
}
