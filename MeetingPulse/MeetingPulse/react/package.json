{
    "name": "OCEPlugin",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "build": "oce-apps-scripts build-ios",
        "start": "oce-apps-scripts start-ios",
        "test": "jest"
    },
    "dependencies": {
        "@babel/core": "*",
        "@reduxjs/toolkit": "1.6.2",
        "apollo-react-native": "git+https://${USER_NAME}:${APOLLO_TOKEN}@gitlab.ims.io/apollo/apollo-react-native#15.0.1",
        "jest": "^26.6.3",
        "jest-file-snapshot": "0.5.0",
        "luxon": "2.2.0",
        "oce-apps-bridges": "15.0.2",
        "oce-apps-scripts": "2.0.1",
        "react": "17.0.2",
        "react-native": "0.66.2",
        "react-native-vector-icons": "6.5.0",
        "react-redux": "7.2.6",
        "react-test-renderer": "17.0.1"
    },
    "jest": {
        "preset": "react-native",
        "collectCoverageFrom": [
            "App.{js,jsx}",
            "src/**/*.{js,jsx}",
            "!<rootDir>/node_modules/",
            "!<rootDir>/bridge/"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!react-native|@react-native|victory-*|@react-native-community|apollo-react-native|react-navigation|@react-navigation|react-naive-gesture-handler|oce-apps-bridges)"
        ],
        "setupFiles": [
            "./node_modules/oce-apps-bridges/lib/setupMocks.js"
        ],
        "setupFilesAfterEnv": [
            "./jest.setup.js"
        ]
    },
    "devDependencies": {
        "@testing-library/jest-native": "^4.0.4",
        "@testing-library/react-hooks": "^7.0.2",
        "@testing-library/react-native": "9.0.0"
    }
}
