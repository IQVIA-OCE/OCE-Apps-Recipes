{
  "name": "OCEPlugin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "oce-apps-scripts build-ios",
    "build-web": "oce-apps-scripts build-web",
    "start": "oce-apps-scripts start-ios",
    "start-web": "oce-apps-scripts start-web",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "*",
    "@react-navigation/native": "6.0.13",
    "@react-navigation/stack": "6.3.2",
    "@reduxjs/toolkit": "1.8.0",
    "@testing-library/jest-native": "4.0.4",
    "@testing-library/react-native": "9.0.0",
    "apollo-react-native": "git+https://${USER_NAME}:${APOLLO_TOKEN}@gitlab.ims.io/apollo/apollo-react-native#${APOLLO_VERSION}",
    "jest": "26.6.3",
    "jest-file-snapshot": "0.5.0",
    "luxon": "2.3.1",
    "oce-apps-bridges": "16.0.0",
    "oce-apps-scripts": "3.0.0",
    "react": "18.1.0",
    "react-native": "0.70.3",
    "react-native-gesture-handler": "2.8.0",
    "react-native-vector-icons": "6.4.2",
    "react-redux": "7.2.6",
    "react-test-renderer": "18.2.0",
    "babel-preset-expo": "9.2.0"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "App.{js,jsx}",
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/bridge/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-native|victory-*|@react-native-community|apollo-react-native|react-navigation|@react-navigation|react-native-gesture-handler|oce-apps-bridges)"
    ],
    "setupFiles": [
      "./node_modules/oce-apps-bridges/lib/setupMocks.js"
    ]
  }
}
