{
  "name": "OCEPlugin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "oce-apps-scripts build-ios",
    "start": "oce-apps-scripts start-ios",
    "build-web": "oce-apps-scripts build-web",
    "start-web": "oce-apps-scripts start-web",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "*",
    "@react-navigation/core": "6.4.0",
    "@react-navigation/native": "6.0.13",
    "@react-navigation/stack": "6.3.2",
    "@testing-library/react-native": "11.0.0",
    "apollo-react-native": "git+https://${USER_NAME}:${APOLLO_TOKEN}@gitlab.ims.io/apollo/apollo-react-native#${APOLLO_VERSION}",
    "babel-preset-expo": "9.2.0",
    "jest": "26.6.3",
    "jest-file-snapshot": "0.5.0",
    "oce-apps-bridges": "17.0.5",
    "oce-apps-scripts": "4.0.0",
    "react": "18.1.0",
    "react-native": "0.70.3",
    "react-native-gesture-handler": "2.8.0",
    "react-native-renderer": "15.5.3",
    "react-native-screens": "3.4.0",
    "react-native-vector-icons": "6.4.2",
    "react-native-webview": "11.14.1",
    "react-test-renderer": "18.2.0",
    "throttle-debounce": "2.1.0",
    "ts-jest": "24.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.8.0"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverageFrom": [
      "App.{js,jsx}",
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/bridge/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-native|victory-*|@react-native-community|apollo-react-native|react-navigation|@react-navigation|react-naive-gesture-handler|oce-apps-bridges|react-native-vector-icons)"
    ],
    "setupFiles": [
      "./node_modules/oce-apps-bridges/lib/setupMocks.js",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ]
  }
}
